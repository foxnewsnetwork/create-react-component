{
  "name": "react-component-boilerplate",
  "version": "1.0.0",
  "description": "A simple react component boilerplate using webpack v2",
  "main": "src/index.js",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^0.26.1",
    "jest": "^18.1.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tools": "^0.13.3",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "scripts": {
    "server": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "watch": "./node_modules/.bin/webpack --watch --config=webpack.build.config.js",
    "build": "./node_modules/.bin/webpack --config=webpack.build.config.js",
    "pre-deploy": "./node_modules/.bin/webpack --config=webpack.config.js",
    "deploy": "npm run pre-deploy && git subtree push --prefix demo origin gh-pages",
    "start": "npm run build && npm run server",
    "test": "jest"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "/node_modules/react",
      "/node_modules/react-dom",
      "/node_modules/react-addons-test-utils"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/preprocessor.js"
    }
  },
  "peerDependencies": {
    "react-dom": "^15.4.2"
  },
  "repository": "https://github.com/gokulkrishh/react-component-boilerplate",
  "author": "Gokulakrishnan Kalaikovan",
  "license": "MIT"
}
